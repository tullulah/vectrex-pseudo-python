[project]
name = "test_noise"
version = "0.1.0"
author = ""
description = "Minimal noise test"
entry = "src/main.vpy"

[build]
output = "build/test_noise.bin"
target = "vectrex"
optimization = 0
debug_symbols = true

[sources]
vpy = ["src/**/*.vpy"]

[resources]
vectors = ["assets/vectors/**/*.vec"]
music = ["assets/music/**/*.vmus"]
sfx = ["assets/sfx/**/*.vmus"]
